{
	"name": "Skribbl.io Image Reference",
	"version": "1.0",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["*://Skribbl.io/*"],
			"js": ["content.js"]
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": ""
	},
	"permissions": ["tabs"],
	"background": {
		"scripts": ["background.js"]
	}
}